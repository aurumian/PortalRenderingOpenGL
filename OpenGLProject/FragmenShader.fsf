#version 330 core

in vec3 ourColor; 
in vec2 TexCoord;

uniform sampler2D texture1;
uniform sampler2D texture2;
uniform float mixAlpha;

out vec4 FragColor; 

void main() {
	//vec2 TexCoord2 = vec2(-TexCoord.x, TexCoord.y);
	//vec2 TexCoord2 = TexCoord * 0.5f + 0.25;
    FragColor = mix(texture(texture1, TexCoord), texture(texture2, TexCoord), mixAlpha);
}
