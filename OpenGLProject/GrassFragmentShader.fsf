#version 440 core
struct Material{
	sampler2D texture_diffuse0;
};

in VS_OUT {
    vec3 FragPos;
    vec3 Normal;
    vec2 texCoord;
    vec4 FragPosLightSpace;
} fs_in;


uniform Material material;

out vec4 FragColor;

void main() {
	vec4 color = texture(material.texture_diffuse0, fs_in.texCoord);
	if (color.a < 0.1f)
		discard;
	FragColor = color;
}